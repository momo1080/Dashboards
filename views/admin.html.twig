                            <div class="info ">
                                <h3 class="margin-textmonlogo">Fatigué</h3>
                            </div>
                        </div>
                    </div>
                    <div class=" col-12 col-sm-12 col-md-4 col-lg-4 col-xl-4">
                        <div class="box">
                            <img class="mon_logoadmin margin-monlogoadmin" src="{{subProject}}/media/stresséadmin.png" alt=""
                                width="80" height="80">
                            <div class="info">
                                <h3 class="margin-textmonlogo">Stressé</h3>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
