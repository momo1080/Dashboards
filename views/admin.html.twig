    <section id="contents">

        <div class="welcome">
            <div class="container-fluid">
                <div class="row content">
                    <div class=" col-10 col-sm-6 col-md-10 col-lg-10 col-xl-10">
                        <div class="content">
                            <h2 class="text-white datetime">date </h2>
                        </div>
                    </div>
                    <div class=" col-2 col-sm-6 col-md-2 col-lg-2 col-xl-2">
                        <div class="content butoon_logout">
                            <h2 class="h5">Logout</h2>
                            <a class="logo text-center " href="{{subProject}}/logout">
                                <img class="lagout_button" src="{{subProject}}/media/ui.png" alt="" width="35" height="35">
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <section class="statistics  ">
            <div class="container-fluid ">
                <div class=" row ">
                    <div class=" col-12 col-sm-12 col-md-4 col-lg-4 col-xl-4">
                        <div class="box">
                            <img class="mon_logoadmin margin-monlogoadmin" src="{{subProject}}/media/heureuxadmin.png" alt=""
