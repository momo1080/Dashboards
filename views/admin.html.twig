                        <div id="graph-jour" class="col-md-12 mon-d-none">
                            <div class="chart-container">
                                <h3>Humeur du jour <span class="nom-service"></span></h3>
                                <canvas id="myChart3"></canvas>
                            </div>
                        </div>

                        <div id="graph-mois" class="col-md-12">
                            <div class="chart-container">
                                <h3>Humeur du mois</h3>
                                <canvas id="myChart"></canvas>
                            </div>
                        </div>
                        <section class="statistics ">
                            <div class="container-fluid ">
                                <div class=" row ">
                                    <div class=" col-12 col-sm-12 col-md-4 col-lg-4 col-xl-4">
                                        <div class="box">
                                            <img class="mon_logoadmin margin-monlogoadmin" src="{{subProject}}/media/heureuxadmin.png"
                                                alt="" width="80" height="80">
                                            <div class="info">
                                                <h3 class="margin-textmonlogo">Heureux</h3>
                                            </div>

                                        </div>
                                    </div>

                                    <div class="col-12 col-sm-12 col-md-4 col-lg-4 col-xl-4">
                                        <div class="box">
                                            <img class="mon_logoadmin margin-monlogoadmin" src="{{subProject}}/media/fatiguéadmin.png"
