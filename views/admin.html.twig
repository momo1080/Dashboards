                                            <div class="info ">
                                                <h3 class="margin-textmonlogo">Fatigué</h3>
                                            </div>
                                        </div>
                                    </div>
                                    <div class=" col-12 col-sm-12 col-md-4 col-lg-4 col-xl-4">
                                        <div class="box">
                                            <img class="mon_logoadmin margin-monlogoadmin" src="{{subProject}}/media/stresséadmin.png"
                                                alt="" width="80" height="80">
                                            <div class="info">
                                                <h3 class="margin-textmonlogo">Stressé</h3>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <!-- humeur par jour dans le mois / crise cardiaque -->
                        <div id="graph-cardio" class="col-md-12">
                            <div class="chart-container">
                                <h3>Humeur par jour du mois <span class="nom-service"></span></h3>
                                <canvas id="myChart2"></canvas>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>

    </section>

    <script src='https://code.jquery.com/jquery-latest.js'></script>
