                            <div class="info">
                                <h3 class="margin-textmonlogo">Heureux</h3>
                            </div>

                        </div>
                    </div>

                    <div class="col-12 col-sm-12 col-md-4 col-lg-4 col-xl-4">
                        <div class="box">
                            <img class="mon_logoadmin margin-monlogoadmin" src="{{subProject}}/media/fatiguéadmin.png" alt=""
